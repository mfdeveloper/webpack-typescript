    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true,
    "printWidth": 80,
    "endOfLine": "auto",
    "allowParens": "avoid",
    "overrides": [
        {
            "files": ".prettierrc",
            "options": { "parser": "json" }
        }
    ]
}
